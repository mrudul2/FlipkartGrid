{
    "version": 2,
    "builds": [{ "src": "*.js", "use": "@vercel/node" }],
    "rewrites": [
        { "source": "/(.*)", "destination": "src/app.js" }
    ],
    "routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "https://syntax-terminators-hosting-api.vercel.app/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
}
